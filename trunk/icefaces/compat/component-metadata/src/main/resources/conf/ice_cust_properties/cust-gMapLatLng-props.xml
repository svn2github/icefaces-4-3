<!--
&ice-att-userrole-enabled-props;
&ice-att-userrole-rendered-props;
-->
&base-props;
<property>
    <description>
        The longitude
    </description>
    <property-name>longitude</property-name>
    <property-class>java.lang.String</property-class>
    <property-extension>
        <category>ADVANCED</category>
        <default-value>-122.1419</default-value>
        <editor-class>
            &stringEditor;
        </editor-class>
    </property-extension>
</property>
<property>
    <description>
        The latitude
    </description>
    <property-name>latitude</property-name>
    <property-class>java.lang.String</property-class>
    <property-extension>
        <category>ADVANCED</category>
        <default-value>37.4419</default-value>
        <editor-class>
            &stringEditor;
        </editor-class>
    </property-extension>
</property>
        