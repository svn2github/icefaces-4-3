&uicommand-props;
&ice-att-partialsubmit-props;
&link-props;
<property>
    <description>
        <![CDATA[
		The base name for all style classes. Default value is <b>iceCmdLnk</b> if enabled
		and <b>iceCmdLnk-dis</b> if disabled.
		]]>		
    </description>
    <property-name>styleClass</property-name>
    <property-class>java.lang.String</property-class>
    <property-extension>
        <category>ADVANCED</category>
        <editor-class>
            &styleClassEditor;
        </editor-class>
    </property-extension>
</property>

<property>
    <description>
        <![CDATA[
		The id of the ice:panelConfirmation component associated to this link.
		]]>
    </description>
    <property-name>panelConfirmation</property-name>
    <property-class>java.lang.String</property-class>
    <property-extension>
        <category>ADVANCED</category>
        <editor-class>
            &styleClassEditor;
        </editor-class>
    </property-extension>
</property>
