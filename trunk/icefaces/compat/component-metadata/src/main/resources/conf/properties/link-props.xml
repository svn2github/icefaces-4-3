<property>
    <description>
		Name of a frame where the resource retrieved via this hyperlink is to 
		be displayed.
    </description>
    <property-name>target</property-name>
      <property-extension>
        <category>APPEARANCE</category>
        <editor-class>
           &selectOneDomainEditor;
        </editor-class>
        <named-value>
          <name>&domainClass;</name>
          <expression>
            &frameTargetsDomain;
          </expression>
        </named-value>
      </property-extension>
</property>


<property>
    <description>
		The content type of the resource designated by this hyperlink.
    </description>
    <property-name>type</property-name>
      <property-extension>
        <category>ADVANCED</category>
        <editor-class>
         &selectOneDomainEditor;
        </editor-class>
        <named-value>
          <name>&domainClass;</name>
          <expression>
            &mimeTypesDomain;
          </expression>
        </named-value>
      </property-extension>
</property>
