<property>
	<description>
		Position of this element in the tabbing order for the current document. 
		This value must be an integer between 0 and 32767.	
	</description>  

    <property-name>tabindex</property-name>
    <property-extension>
        <category>ADVANCED</category>
        <editor-class>
            &integerEditor;
        </editor-class>
        <named-value>
            <name>minValue</name>
            <expression>new Integer(0)</expression>
        </named-value>
        <named-value>
            <name>maxValue</name>
            <expression>new Integer(Short.MAX_VALUE)</expression>
        </named-value>        
    </property-extension>
</property>