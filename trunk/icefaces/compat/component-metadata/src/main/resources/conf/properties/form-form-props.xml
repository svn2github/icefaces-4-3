    <property>
      <property-name>accept</property-name>
      <property-extension>
        <category>ADVANCED</category>
        <editor-class>
         &selectOneDomainEditor;
        </editor-class>
        <named-value>
          <name>&domainClass;</name>
          <expression>
            &MimeTypesDomain;
          </expression>
        </named-value>
      </property-extension>
    </property>

    <property>
      <property-name>acceptcharset</property-name>
      <property-extension>
        <category>ADVANCED</category>
        <editor-class>
          &stringEditor;
        </editor-class>
        <!-- TODO: comma-delimited list of charset values
        <named-value>           
        </named-value>
        -->
      </property-extension>
    </property>

    <property>
      <property-name>enctype</property-name>
      <property-extension>
        <category>ADVANCED</category>
        <editor-class>
           &selectOneDomainEditor;
        </editor-class>
        <named-value>
          <name>&domainClass;</name>
          <expression>
             &MimeTypesDomain;
          </expression>
        </named-value>
      </property-extension>
    </property>

    <property>
      <property-name>onreset</property-name>
      <property-class>java.lang.String</property-class>
      <property-extension>
          <category>JAVASCRIPT</category>
          <editor-class>
            &javascriptEditor;
          </editor-class>
      </property-extension>
    </property>

    <property>
      <property-name>onsubmit</property-name>
      <property-class>java.lang.String</property-class>
      <property-extension>
          <category>JAVASCRIPT</category>
          <editor-class>
            &javascriptEditor;
          </editor-class>
      </property-extension>
    </property>



<property>
      <property-name>target</property-name>
      <property-extension>
        <category>APPEARANCE</category>
        <editor-class>
           &selectOneDomainEditor;
        </editor-class>
        <named-value>
          <name>&domainClass;</name>
          <expression>
            &frameTargetsDomain;
          </expression>
        </named-value>
      </property-extension>
    </property>
